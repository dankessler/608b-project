	howpublished = {Poster},
	note = {{UMich} Psychiatry Silverman Conference},
	author = {Kessler, Daniel A. and Fang, Yu and Angstadt, Michael and Prem Kumar, Krishan and Welsh, Robert and Sripada, Chandra},
	date = {2013-06-19},
	keywords = {{CVother}},
	file = {Kessler et al. - 2013 - Aberrant connectomic architecture of the resting b.pdf:C\:\\Users\\danke\\Zotero\\storage\\W45BJQCQ\\Kessler et al. - 2013 - Aberrant connectomic architecture of the resting b.pdf:application/pdf}
}

@unpublished{kessler_growth_2016,
	location = {Ann Arbor, {MI}},
	title = {Growth Charting of Brain Connectivity Networks and the Identification of Attention Impairment in Youth},
	abstract = {Background
Growth charts of physical characteristics (e.g., pediatric growth charts of height \& head circumference) have long been used for early identification of somatic abnormalities. In the present study, we developed growth charts for maturing functional brain networks in order to identify clinically-relevant neurocognitive abnormalities, in particular impairments in attention functioning. The human brain is organized into a number of interconnected units called intrinsic connectivity networks ({ICNs}), and {ICNs} exhibit extensive development and differentiation during youth. In addition, {ICNs} are also implicated in attention performance. These observations raise an intriguing hypothesis: Deviations from neurotypical patterns of network maturation, calculated by means of network growth charts, could reliably predict the early emergence of sustained attention impairment. This study is, as far as we know, the first to investigate this hypothesis. 


Methods
Resting state functional neuroimaging scans were obtained for 519 youth, age 8-22. These scans were used to produce comprehensive maps of functional connectivity across the whole brain (``functional connectomes''). All participants also performed the Penn Continuous Performance Test, which evaluates sustained attention functioning. Using a multivariate statistical method (joint independent component analyses) we produced growth charts that represent neurotypical patterns of maturation for major brain networks. ``Underdevelopment scores'' were then calculated reflecting each participant's deviations from neurotypical growth. These scores were used to predict attention dysfunction (as measured by the Penn Continuous Performance Task).

Results
Several key networks showed evidence of robust maturation including default mode network and several task-positive networks, which have previously been implicated in attention functioning. After constructing growth charts for these networks, we found that underdevelopment scores (i.e., scores reflecting underdevelopment of networks relative to age) predicted a remarkable 28.7\% of the variance across participants in attention functioning (F(15,503) = 13.48, p {\textless} 2.2 * 10{\textasciicircum}(-16))). Moreover, they served as a reliable biomarker of severe attention impairment (79.3\% prediction accuracy). 

Conclusions
This study introduces a novel brain network growth charting method which we used to produce a highly sensitive brain-based biomarker of attention dysfunction. Growth charting of functional brain networks could provide a new approach for constructing neuroimaging biomarkers for other psychiatrically relevant conditions. Extensions of this methodology for biomarker construction for psychosis- and autism-spectrum traits are underway.




Using this technique, we showed children who perform substantially worse in objective tests of attention performance are underedeveloped with respect to key brain networks implicated in attention functioning perform substantially worse in objective tests of attention performance. produced a highly sensitive brain-based biomarker of attention dysfunction by growth charting brain networks implicated in both developmental and attention literature. We believe this approach holds great promise for biomarker discovery in other developmentally emergent cognitive domains.},
