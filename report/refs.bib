@unpublished{sripada_whole-brain_2013,
	location = {San Francisco, {CA}},
	title = {Whole-Brain Connectomic Analysis of 145 Resting State Scans Reveals Network Neurosignatures of Schizophrenia},
	url = {http://www.abstractsonline.com/Plan/ViewAbstract.aspx?sKey=0b7724d2-8f52-4366-8526-993d4e673c5e&cKey=424fdcf4-f4ad-4aeb-af50-d07f78bc6a14&mKey=%7b5D0375C3-8DF6-45CA-9F2B-467FE341BC0C%7d},
	abstract = {Background: Schizophrenia is a neurodevelopment disorder with extensive evidence of neural disconnection. In order to further characterize connectivity abnormalities in schizophrenia, we applied a novel whole-brain connectomic approach to 145 resting state scans from the Center for Biomedical Research Excellence sample.
Methods: 71 patients with schizophrenia ({SZ}) and 74 healthy controls ({HC}) underwent 6-minute resting scans. After standard pre-processing, 1166 {ROIs} were placed at 12mm intervals across the brain, and cross-correlation matrices (connectomes') were produced. These were thresholded and overlaid on the Yeo (2012) network parcellation of the resting brain to implicate seven major resting state networks, and five additional subcortical regions (see Figure). Cross-tabulations between networks were compared to a null distribution with difference of proportion Z-tests in order to identify deviant intra- and inter-network connectivity in {SZ} versus {HC}. Motion regressors at the first and second-levels quantified and minimized motion effects.
Results: {SZ} brains exhibited diffuse hypoconnectivity within the default network (Z=4.01, p{\textless}0.001), and between hippocampus/parahippocampus and default and attention networks (Z=7.06, p{\textless}0.001). Hyperconnectivity was observed within frontoparietal networks (Z=3.41, p{\textless}0.001), and between other task-positive networks, including dorsal attention, ventral attention, and somatomotor network (ps{\textless}0.001; see Figure).
Conclusions: Using novel, whole-brain connectomic methods, we provide evidence of robust, distributed connectivity abnormalities in default, attention, and limbic networks in schizophrenia that could potentially serve as reliable biomarkers of disease.},
	type = {Talk},
	howpublished = {Talk},
	note = {Society of Biological Psychiatry Annual Meeting},
	author = {Sripada, Chandra and Kessler, Daniel A. and Watanabe, Takanori and Welsh, Robert and Fang, Yu and Angstadt, Michael and Taylor, Stephen and Scott, Clayton},
	date = {2013-05-16},
	keywords = {{CVother}}
}

@unpublished{kessler_attention_2013,
	location = {Ann Arbor, {MI}},
	title = {Attention, Methylphenidate, and Task Performance in the Dynamic, Networked Brain},
	abstract = {Methylphenidate is a psychostimulant medication that reliably produces acute improvements in attention functioning. Recent models of attention focus on interconnectivity between large-scale networks, especially default network and task positive networks. However, the effects of methylphenidate in altering network connectivity patterns are poorly understood. We conducted a within-subject, double-blind, placebo-controlled, randomized, counterbalanced, cross-over study, in which 32 healthy volunteers received either methylphenidate or placebo prior to two {fMRI} scans separated by approximately one week. We report the results of three analyses of data generated from this study. Analysis 1 used paired support-vector machines, a multivariate classification method for within-subject data developed by our group, applied to resting state scans. We demonstrate methylphenidate produces a reliably detected multivariate neurosignature. In particular, methylphenidate generates altered default network-task positive network coupling, and altered connectivity within visual and somatomotor networks. Analysis 2 used network contingency analysis, a novel connectomic analysis method developed by our group, to identify network relationship patterns that predict across session changes in reaction time variability. Analysis 3 introduces dynamic connectivity, a new method for investigating changes in network coupling patterns over short time intervals (e.g., 15-60 seconds) within a scanning session. We demonstrate that changes in dynamic connectivity between large-scale networks predict changes in reaction time variability across short temporal epochs within a single scan.},
	type = {Talk},
	howpublished = {Talk},
	note = {{UMich} {fMRI} Symposium},
