	file = {arXiv\:1305.5782 PDF:C\:\\Users\\danke\\Zotero\\storage\\K8C2GYB8\\Aicher et al. - 2013 - Adapting the Stochastic Block Model to Edge-Weight.pdf:application/pdf}
